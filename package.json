{
	"name": "srocket",
	"version": "1.4.0",
	"scripts": {
		"test": "jest --config=jest.json",
		"test:watch": "npm run jest -- --watch",
		"docs:build": "typedoc --out ./docs/apidoc",
		"docs:serve": "docsify serve ./docs",
		"prettier": "prettier --write ./**/*.ts",
		"start": "nodemon --exec ts-node ./samples/Base/Server.ts",
		"release": "node ./scripts/deploy.js",
		"precommit": "lint-staged"
	},
	"lint-staged": {
		"*.ts": [
			"./node_modules/.bin/prettier --write",
			"git add"
		]
	},
	"main": "./index.js",
	"types": "./index.d.ts",
	"dependencies": {
		"class-validator": "0.8.5",
		"inversify": "4.13.0",
		"inversify-inject-decorators": "^3.1.0",
		"reflect-metadata": "0.1.12",
		"socket.io": "2.1.1",
		"socket.io-client": "2.1.1",
		"socketio-wildcard": "2.0.0",
		"typescript": "2.9.2",
		"validator": "10.4.0"
	},
	"devDependencies": {
		"@types/debug": "0.0.30",
		"@types/jest": "23.1.2",
		"@types/lodash": "4.14.110",
		"@types/node": "10.3.6",
		"@types/readline-sync": "1.4.3",
		"@types/semver": "5.5.0",
		"@types/socket.io": "1.4.36",
		"@types/socket.io-client": "1.4.32",
		"@types/validator": "9.4.1",
		"chalk": "2.4.1",
		"docsify": "4.6.10",
		"docsify-cli": "4.2.1",
		"handy-redis": "1.3.0",
		"husky": "0.14.3",
		"jest": "23.2.0",
		"lint-staged": "^7.2.0",
		"lodash": "4.17.10",
		"mock-socket": "7.1.0",
		"prettier": "1.13.6",
		"readline-sync": "1.4.9",
		"semver": "5.5.0",
		"shelljs": "0.8.2",
		"ts-jest": "22.4.6",
		"ts-node": "7.0.0",
		"tsconfig-paths": "3.4.1",
		"typedoc": "^0.11.1"
	},
	"engines": {
		"node": ">=6.0.0"
	},
	"description": "A opinionated typesafe socket.io framework",
	"author": "Patrick Hollweck <patrick-hollweck@gmx.de>",
	"homepage": "https://fetzenrndy.github.io/SRocket/#/",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/FetzenRndy/SRocket/issues",
		"email": "patrick-hollweck@gmx.de"
	},
	"repository": {
		"type": "GitHub",
		"url": "https://github.com/FetzenRndy/SRocket"
	},
	"keywords": [
		"socket.io",
		"srocket",
		"realtime",
		"typescript",
		"framework"
	]
}